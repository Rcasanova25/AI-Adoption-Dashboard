# AI Adoption Dashboard Environment Variables
# Copy this file to .env and update with your actual values

# Path to AI adoption resources directory
# For Windows (WSL): /mnt/c/Users/username/path/to/AI adoption resources
# For Linux: /home/username/path/to/AI adoption resources
AI_ADOPTION_RESOURCES_PATH=/path/to/your/AI adoption resources

# Cache configuration
CACHE_MEMORY_SIZE=200
CACHE_MEMORY_TTL=600
CACHE_DISK_SIZE=2147483648  # 2GB in bytes
MAX_WORKERS=4

# Logging configuration
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# Application settings
DEBUG=False
SECRET_KEY=your-secret-key-here

# API settings
API_TIMEOUT=30